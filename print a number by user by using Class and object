import java.util.*;
public class Class_Object1 {
                    int x;
                    public static void main(String args[]){
                                        Class_Object1 ob=new Class_Object1();//object creation 
                                        Scanner sc=new Scanner(System.in);
                                        System.out.println("enter the value of elemt =");
                                        ob.x=sc.nextInt();
                                        System.out.println("return value = "+ ob.x);

                    }
}
