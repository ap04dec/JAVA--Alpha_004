package Sorting;
import java.util.*;
public class inbuilt_sort {

    public static void Arr(int arr[]) 
    {
        for(int i=0; i<arr.length;i++){
            System.out.print(arr[i]+ " ");
        }
        
    }
    public static void main(String args[]) {
        int arr[] ={1,4,3,232,32};
        Arrays.sort(arr);
        
        Arr(arr);
        
    }
}
