public class function_bt_arguments {
                    int x,y;// instance variable
       void show(int p,int q)
       {
                    x=p;
                    y=q;
                    System.out.println("multipliication of "+ p +" and " + q +" = "+ x*y);
       }
       public static void main(String args[])
       {
                    function_bt_arguments on= new function_bt_arguments();
                    on.show(4, 4);
       }
                    
}
