import java.util.*;
public class typeSafearray
{
    public static void main(String args[])
    {
        ArrayList <String> names = new ArrayList <String>();
        names.add("anil");
        names.add("ankit");
        System.out.println(names);
        
		Iterator itr= names.iterator();
		while(itr.hasNext())
		{
					System.out.println(itr.next());
		}
    }
}
