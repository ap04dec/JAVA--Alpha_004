import java.util.Scanner;

public class function_bt_arguments {
                    int x,y;// instance variable
       void show(int p,int q)
       {   Scanner sc=new Scanner(System.in);
                    System.out.println("enter the value of p = ");
                    p=sc.nextInt();
                    System.out.println("enter the value of q= ");
                    q=sc.nextInt();
                    x=p;
                    y=q;
                    System.out.println("multipliication of "+ p +" and " + q +" = "+ x*y);
       }
       public static void main(String args[])
       {            
                    function_bt_arguments on= new function_bt_arguments();
                    on.show(0, 0);
                  
       }
                    
}
